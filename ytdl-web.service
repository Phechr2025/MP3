[Unit]
Description=YouTube Downloader Web (v1.0)
After=network-online.target

[Service]
Type=simple
WorkingDirectory=/root/ytweb
Environment="PYTHONUNBUFFERED=1"
ExecStart=/root/ytweb/venv/bin/gunicorn --workers 2 --bind 0.0.0.0:8090 app:app
Restart=always
RestartSec=3

[Install]
WantedBy=multi-user.target
